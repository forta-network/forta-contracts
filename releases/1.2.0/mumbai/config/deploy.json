            "init-args": ["deployment.access-manager", "2", "86400"],
            "opts": {
                "unsafe-allow": [
                    "delegatecall"
                ],
                "constructor-args": [
                    "deployment.forwarder", "deployment.forta", "deployment.stake-subject-gateway"
                ]
            }
            
        }
    },
    "StakeAllocator": {
        "impl": {
